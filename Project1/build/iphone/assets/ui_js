var format1={color:"fff",top:40,font:{fontSize:60}},format2={color:"fff",top:40,font:{fontSize:40}},buildUI=function(e){var a=Ti.UI.createWindow({backgroundImage:"background.png",backgroundRepeat:"repeat-y",layout:"vertical"}),o=Ti.UI.createLabel(format2);o.text=e.month+"/"+e.day+"/"+e.year;var t=Ti.UI.createLabel(format2);t.text=e.time;var r=Ti.UI.createLabel(format2);r.text=e.city+", "+e.country;var i=Ti.UI.createLabel(format1);i.text="High: "+e.high+", Low: "+e.low;var n=Ti.UI.createLabel(format1);n.text="Conditions: "+e.conditions;var c=Ti.UI.createLabel(format1);c.text="Average: "+e.aveWind+" mph";var d=Ti.UI.createLabel(format1);d.text="Max: "+e.maxWind+" mph",a.add(o),a.add(t),a.add(r),a.add(i),a.add(n),a.add(c),a.add(d),a.open()};exports.buildUI=buildUI;