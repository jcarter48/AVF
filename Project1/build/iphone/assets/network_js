var dataPull=function(e,o){var a="http://api.wunderground.com/api/465b4ffeedaac08a/forecast/geolookup/q/"+e+","+o+".json";if(Ti.Network.online){var t=Ti.Network.createHTTPClient({onload:function(e){var o=JSON.parse(this.responseText),a={icon:o.forecast.simpleforecast.forecastday[0].icon_url,conditions:o.forecast.simpleforecast.forecastday[0].conditions,city:o.location.city,country:o.location.country_name,time:o.forecast.txt_forecast.date,high:o.forecast.simpleforecast.forecastday[0].high.fahrenheit,low:o.forecast.simpleforecast.forecastday[0].low.fahrenheit,month:o.forecast.simpleforecast.forecastday[0].date.month,day:o.forecast.simpleforecast.forecastday[0].date.day,year:o.forecast.simpleforecast.forecastday[0].date.year,aveWind:o.forecast.simpleforecast.forecastday[0].avewind.mph,maxWind:o.forecast.simpleforecast.forecastday[0].maxwind.mph},t=require("ui");t.buildUI(a)}});t.open("GET",a),t.send()}else alert("Network currently unavailable.")};exports.dataPull=dataPull;