Ti.UI.setBackgroundColor("#ffffff");var win=Ti.UI.createWindow({backgroundColor:"#fff",top:20}),viewContainer=Ti.UI.createScrollView({layout:"vertical",showVerticalScrollIndicator:!0}),format1={text:"...",color:"#ffffff",font:{fontSize:32}},format2={text:"...",color:"#ffffff",font:{fontSize:16}};win.add(viewContainer),win.open();var textField=Ti.UI.createTextField({borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,color:"#000",width:250,height:60});viewContainer.add(textField);var textButton=Ti.UI.createButton({title:"GO!",height:60,width:250});textButton.addEventListener("click",function(){var e=require("api");e.getData1(textField.value)}),viewContainer.add(textButton);var buildUI=function(e){for(var t=0,a=e.length;a>t;t++)var i=Ti.UI.createLabel({text:e[t].name+"    "+e[t].leaguePoints+"lp    W/L:"+e[t].wins+"/"+e[t].losses,font:{fontSize:20},bottom:20});viewContainer.add(i)};exports.buildUI=buildUI;