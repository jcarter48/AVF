alert("network");

var stockRequest = function() {
	
	var url = "http://dev.markitondemand.com/MODApis/Api/v2/Quote/json?symbol=AAPL";
	var getData = Ti.Network.createHTTPClient({
		onload : function(){
			
		},
		onerror : function(){
			
		},
		timeout : 3000
	});
	getData.open("GET", url);
	getData.send();
	
};

exports.stockRequest = stockRequest; 