var uiFunction = function(obj){
	alert("ui module");
	
	var window = Ti.UI.createWindow({
		backgroundColor: "ccc"
	});
	var species = Ti.UI.createLabel({
		color: "000",
		text: "Species: " + obj.species,
		top: 20
	});
	var breed = Ti.UI.createLabel({
		color: "000",
		text: "Breed: " + obj.breed,
		top: 20
	});
	var name = Ti.UI.createLabel({
		color: "000",
		text: "Name: " + obj.name,
		top: 20
	});
	var age = Ti.UI.createLabel({
		color: "000",
		text: "Age: " + obj.age,
		top: 20
	});
	
	win.add(species);
	win.add(breed);
	win.add(name);
	win.add(age);
	
	win.open();
};

exports.uiFunction = uiFunction;