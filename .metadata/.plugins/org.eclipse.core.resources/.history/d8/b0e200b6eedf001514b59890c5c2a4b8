var buildUI = function(dataObj) {
	var win = Ti.UI.createWindow({
		backgroundColor: "ccc"
	});
	var name = Ti.UI.createLabel({
		color: "000",
		text: dataObj.name
	});
	var symbol = Ti.UI.createLabel({
		color: "000",
		text: dataObj.symbol
	});
	var price = Ti.UI.createLabel({
		color: "000",
		text: dataObj.price
	});
	var hi = Ti.UI.createLabel({
		color: "000",
		text: dataObj.hi
	});
	var lo = Ti.UI.createLabel({
		color: "000",
		text: dataObj.lo
	});
	win.add(name);
	win.add(symbol);
	win.add(price);
	win.add(hi);
	win.add(lo);
	win.open();
};

exports.buildUI = buildUI;
